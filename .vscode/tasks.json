{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang build active file",
            "command": "/usr/bin/clang",
            "args": [
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-g",
                "-I${workspaceFolder}",
                "-I${workspaceFolder}/includes/",
                "-I${workspaceFolder}/builtins/",
